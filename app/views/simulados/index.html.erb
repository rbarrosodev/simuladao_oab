<div class="container">
  <div class="row">
    <div class="col-12 text-center my-5">
      <h1 class="display-4"><i class="fas fa-gavel text-primary"></i> Simulados</h1>
      <div class="row">
        <div class="col-12 text-center my-5">
          <% @simulados.each do |simulado| %>
            <h1 class="my-5"><%= link_to simulado.title, simulado %></h1>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>